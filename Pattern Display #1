#include <stdio.h>
int main() 
{
    int k, i;
    //decreasing portion
    for (k = 1; k <= 5; k++) 
    {
        for (i = 1; i <= 6-k; i++)
        {
            printf("X");
        }
        printf("\n");
    }
    //increasing portion
    for (k = 1; k <= 4; k++) 
    {
        for (i = 1; i <= k+1; i++)
        {
            printf("X");
        }
        printf("\n");
    }
    return 0;
}
