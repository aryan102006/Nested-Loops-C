#include <stdio.h>
int main() 
{
    int i, j, k;
    for (k = 1; k <= 5; k++)
    {
        for (i = 1; i <= (5-k); i++) 
        {
            printf(" ");
        }
        for (j = 1; j <= (2*k - 1); j++)
        {
            printf("%d", j);
        }
        printf("\n");
    }
    return 0;
}
