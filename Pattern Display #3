#include <stdio.h>
int main() 
{
    int i, j, k, p;
    for (k = 1; k <= 5; k++)
    {
        for (i = 1; i <= (5-k); i++) 
        {
            printf(" ");
        }
        for (j = 1; j <= k; j++)
        {
            printf("X");
        }
        for (p = 1; p <= (k-1); p++)
        {
            printf("X");
        }
        printf("\n");
    }
    return 0;
}
